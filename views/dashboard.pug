extends _shared/_master
block content
    // Page content
    #page-content
      // First Row
      .row
        // Simple Stats Widgets
        .col-sm-6.col-lg-3
          a.widget(href='javascript:void(0)')
            .widget-content.widget-content-mini.text-right.clearfix
              .widget-icon.pull-left.themed-background
                i.gi.gi-cardio.text-light-op
              |                                         
              h2.widget-heading.h3
                strong
                  span(data-toggle='counter', data-to='2835')
              |                                         
              span.text-muted SALES
        |                             
        .col-sm-6.col-lg-3
          a.widget(href='javascript:void(0)')
            .widget-content.widget-content-mini.text-right.clearfix
              .widget-icon.pull-left.themed-background-success
                i.gi.gi-user.text-light-op
              |                                         
              h2.widget-heading.h3.text-success
                strong
                  | + 
                  span(data-toggle='counter', data-to='2862')
              |                                         
              span.text-muted NEW USERS
        |                             
        .col-sm-6.col-lg-3
          a.widget(href='javascript:void(0)')
            .widget-content.widget-content-mini.text-right.clearfix
              .widget-icon.pull-left.themed-background-warning
                i.gi.gi-briefcase.text-light-op
              |                                         
              h2.widget-heading.h3.text-warning
                strong
                  | + 
                  span(data-toggle='counter', data-to='75')
              |                                         
              span.text-muted PROJECTS
        |                             
        .col-sm-6.col-lg-3
          a.widget(href='javascript:void(0)')
            .widget-content.widget-content-mini.text-right.clearfix
              .widget-icon.pull-left.themed-background-danger
                i.gi.gi-wallet.text-light-op
              |                                         
              h2.widget-heading.h3.text-danger
                strong
                  | $ 
                  span(data-toggle='counter', data-to='5820')
              |                                         
              span.text-muted EARNINGS
        // END Simple Stats Widgets
      // END First Row
      // Second Row
      .row
        .col-sm-6.col-lg-8
          // Chart Widget
          .widget
            .widget-content.border-bottom
              span.pull-right.text-muted 2013
              |                                         Last Year&apos;s Data
            |                                     
            .widget-content.border-bottom.themed-background-muted
              // Flot Charts (initialized in js/pages/readyDashboard.js), for more examples you can check out http://www.flotcharts.org/
              #chart-classic-dash(style='height: 393px;')
            |                                     
            .widget-content.widget-content-full
              .row.text-center
                .col-xs-4.push-inner-top-bottom.border-right
                  h3.widget-heading
                    i.gi.gi-wallet.text-dark.push-bit
                    |  
                    br
                    small $ 41k
                |                                             
                .col-xs-4.push-inner-top-bottom
                  h3.widget-heading
                    i.gi.gi-cardio.text-dark.push-bit
                    |  
                    br
                    small 17k Sales
                |                                             
                .col-xs-4.push-inner-top-bottom.border-left
                  h3.widget-heading
                    i.gi.gi-life_preserver.text-dark.push-bit
                    |  
                    br
                    small 3k+ Tickets
          // END Chart Widget
        |                             
        .col-sm-6.col-lg-4
          // Stats User Widget
          a.widget(href='page_ready_profile.html')
            .widget-content.border-bottom.text-dark
              span.pull-right.text-muted This week
              |                                         Featured Author
            |                                     
            .widget-content.border-bottom.text-center.themed-background-muted
              img.img-circle.img-thumbnail.img-thumbnail-avatar-2x(src='/static/img/placeholders/avatars/avatar13@2x.jpg', alt='avatar')
              |                                         
              h2.widget-heading.h3.text-dark Anna Wigren
              |                                         
              span.text-muted
                strong Logo Designer
                | , Sweden
            |                                     
            .widget-content.widget-content-full-top-bottom
              .row.text-center
                .col-xs-6.push-inner-top-bottom.border-right
                  h3.widget-heading
                    i.gi.gi-briefcase.text-dark.push-bit
                    |  
                    br
                    small 35 Projects
                |                                             
                .col-xs-6.push-inner-top-bottom
                  h3.widget-heading
                    i.gi.gi-heart_empty.text-dark.push-bit
                    |  
                    br
                    small 5.3k Likes
          // END Stats User Widget
          // Mini Widgets Row
          .row
            .col-xs-6
              a.widget(href='javascript:void(0)')
                .widget-content.themed-background-info.text-light-op.text-center
                  .widget-icon
                    i.fa.fa-facebook
                |                                             
                .widget-content.text-dark.text-center
                  strong 98k
                  br
                  | Followers
            |                                     
            .col-xs-6
              a.widget(href='javascript:void(0)')
                .widget-content.themed-background-danger.text-light-op.text-center
                  .widget-icon
                    i.fa.fa-database
                |                                             
                .widget-content.text-dark.text-center
                  strong 15
                  br
                  |                                                 Active Servers
          // END Mini Widgets Row
      // END Second Row
      // Third Row
      .row
        .col-sm-4
          // Project Widget
          .widget
            .widget-content.border-bottom
              span.pull-right.text-muted 60%
              |                                         Project
            |                                     
            a.widget-content.themed-background-muted.text-right.clearfix(href='javascript:void(0)')
              img.img-circle.img-thumbnail.img-thumbnail-avatar.pull-left(src='/static/img/placeholders/avatars/avatar6.jpg', alt='avatar')
              |                                         
              h2.widget-heading.h3.text-muted Logo
              |                                         
              .progress.progress-striped.progress-mini.active
                .progress-bar.progress-bar-success(role='progressbar', aria-valuenow='85', aria-valuemin='0', aria-valuemax='100', style='width: 85%')
            |                                     
            a.widget-content.themed-background-muted.border-top.text-right.clearfix(href='javascript:void(0)')
              img.img-circle.img-thumbnail.img-thumbnail-avatar.pull-left(src='/static/img/placeholders/avatars/avatar2.jpg', alt='avatar')
              |                                         
              h2.widget-heading.h3.text-muted Icon Pack
              |                                         
              .progress.progress-striped.progress-mini.active
                .progress-bar.progress-bar-danger(role='progressbar', aria-valuenow='40', aria-valuemin='0', aria-valuemax='100', style='width: 40%')
            |                                     
            a.widget-content.themed-background-muted.border-top.text-right.clearfix(href='javascript:void(0)')
              img.img-circle.img-thumbnail.img-thumbnail-avatar.pull-left(src='/static/img/placeholders/avatars/avatar11.jpg', alt='avatar')
              |                                         
              h2.widget-heading.h3.text-muted Design
              |                                         
              .progress.progress-striped.progress-mini.active
                .progress-bar.progress-bar-warning(role='progressbar', aria-valuenow='55', aria-valuemin='0', aria-valuemax='100', style='width: 55%')
            |                                     
            .widget-content.widget-content-full.border-top
              .row.text-center
                .col-xs-6.push-inner-top-bottom.border-right
                  i.fa.fa-check-circle-o
                  |  On Time
                |                                             
                .col-xs-6.push-inner-top-bottom
                  i.fa.fa-clock-o
                  |  17 Days
          // END Project Widget
        |                             
        .col-sm-4
          // Statistics Widget
          .widget
            .widget-content.border-bottom
              span.pull-right.text-muted
                i.fa.fa-check
              |                                         Weekly Sales
            |                                     
            .widget-content.border-bottom.themed-background-muted.text-center
              span#widget-dashchart-sales 12,15,14,18,16,15,16,17
            |                                     
            .widget-content.widget-content-full-top-bottom.border-bottom
              .row.text-center
                .col-xs-6.push-inner-top-bottom.border-right
                  h3.widget-heading
                    i.gi.gi-book_open.text-dark.push
                    |  
                    br
                    small 123 Total Sales
                |                                             
                .col-xs-6.push-inner-top-bottom
                  h3.widget-heading
                    i.gi.gi-user_add.text-dark.push
                    |  
                    br
                    small +10% Clients
            |                                     
            .widget-content.widget-content-full
              .row.text-center
                .col-xs-6.push-inner-top-bottom.border-right
                  h3.widget-heading
                    i.gi.gi-briefcase.text-dark.push
                    |  
                    br
                    small 5 Products
                |                                             
                .col-xs-6.push-inner-top-bottom
                  h3.widget-heading
                    i.gi.gi-truck.text-dark.push
                    |  
                    br
                    small 10 Deliveries
          // END Statistics Widget
        |                             
        .col-sm-4
          // Story Widget
          .widget
            .widget-content
              span.pull-right.text-muted Featured
              |                                         Story
            |                                     
            .widget-image
              img(src='/static/img/placeholders/photos/photo9.jpg', alt='image')
              |                                         
              .widget-image-content
                h2.widget-heading
                  a.text-light(href='page_ready_article.html')
                    strong The trip that changed my life
                |                                             
                h3.widget-heading.text-light-op.h4 It changed the way I think
              |                                         
              i.gi.gi-airplane
            |                                     
            .widget-content.widget-content-full.text-dark
              .row.text-center
                .col-xs-4.push-inner-top-bottom.border-right
                  i.fa.fa-heart-o
                  |  9.5k
                |                                             
                .col-xs-4.push-inner-top-bottom.border-right
                  i.fa.fa-eye
                  |  76k
                |                                             
                .col-xs-4.push-inner-top-bottom
                  i.fa.fa-share-alt
                  |  7.2k
          // END Story Widget
      // END Third Row
    // END Page Content
