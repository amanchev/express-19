		// Page Container
		// In the PHP version you can set the following options from inc/config file
		// 'boxed' class for a boxed layout
		#page-container.boxed
			// Site Header
			header
				.container
					// Site Logo
					a.site-logo(href='index.html')
						i.fa.fa-cube
						|  myFitness
						strong COACH
					// END Site Logo
					// Site Navigation
					nav
						// Menu Toggle
						// Toggles menu on small screens
						a.btn.btn-default.site-menu-toggle.visible-xs.visible-sm(href='javascript:void(0)') Menu
						// END Menu Toggle
						// Main Menu
						ul.site-nav
							li
								a(href='/home') Get started
							li
								a(href='/pricing') Pricing
							li
								a(href='/trainers/all') Trainers
					// END Site Navigation
			// END Site Header
			// END Site Header
