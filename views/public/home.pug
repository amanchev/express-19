			section.site-section.site-section-top.site-section-light.themed-background-dark-default
				.container
					.push.text-center
						h1.animation-fadeInQuick2Inv
							strong Find Your training inspiration.
						h2.text-light-op.animation-fadeInQuickInv.push-bit
							strong Start using myFitnessCOACH
						a.btn.btn-lg.btn-success.push-right-left(href="/auth/sign-in") 
							strong Sign In
						a.btn.btn-lg.btn-info.push-right-left(href="/auth/sign-up") 
